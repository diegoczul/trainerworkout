{
	"name": "laravel/laravel",
	"description": "The Laravel Framework.",
	"keywords": ["framework", "laravel"],
	"license": "MIT",
	"require": {
		"laravel/framework": "4.2.*",
		"bllim/datatables": "*",
		"nathanmac/laravel-guid": "dev-master",
		"barryvdh/laravel-debugbar": "1.x",
		"jenssegers/date": "*",
		"jasonlewis/expressive-date": "1.0.*",
		
"intervention/image": "2.*",
		"artdarek/oauth-4-laravel": "dev-master",
		"stripe/stripe-php": "~2.1",
		"brainboxlabs/brain-socket": "v1.0.0",
		"guzzlehttp/guzzle": "~4.0",
		"pda/pheanstalk": "2.0.*",
		"hugofirth/mailchimp": "2.0.*",
		"barryvdh/laravel-dompdf": "0.4.*",
		"barryvdh/laravel-snappy": "0.1.x",
		"dimsav/laravel-translatable": "4.4.x",
		"maknz/slack": "^1.7",
    		"daltcore/lara-pdf-merger": "dev-master",
		"glukash/glu-image": "0.*",
		"jenssegers/agent": "^2.5",
		"intercom/intercom-php": "1.0.0",
		 "jeylabs/tcpdi": "1.2"

	},
  "replace": {
    "wfeller/tcpdi": "1.2"
  },
 "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/jeylabs/tcpdi"
    }
  ],	
	"autoload": {
		"classmap": [
			"app/commands",
			"app/controllers",
			"app/models",
			"app/database/migrations",
			"app/database/seeds",
			"app/tests/TestCase.php"
		]
	},
	"require-dev": {
	    "way/generators": "~2.0"
	},
	"scripts": {
		"post-install-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-update-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-create-project-cmd": [
			"php artisan key:generate"
		]
	},
	"config": {
	"preferred-install": "dist",
		"allow-plugins": {
			"kylekatarnls/update-helper": true
		}
	},
	"minimum-stability": "dev"
}
