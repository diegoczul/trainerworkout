body{background:#fff;color:#8e8e8e;font-size:13px;line-height:1.531;font-family: 'Ubuntu', sans-serif; padding: 0 !important}

a{color:#404040;}
a:hover {color:#5897cd;}
p{ margin:0 0 25px 0;}
h1{ margin:0 0 15px 0; color:#5897cd; font-size:39px; font-family: 'Ubuntu', sans-serif; line-height:40px;}
h2{ margin:0 0 10px 0; color:#5897cd; font-size:34px; font-family: 'Ubuntu', sans-serif; line-height:35px;}
h3{ margin:0 0 10px 0; color:#5897cd; font-size:32px; font-family: 'Ubuntu', sans-serif; line-height:30px; font-weight:lighter;}
h4{ margin:0 0 10px 0; color:#5897cd; font-size:28px; font-family: 'Ubuntu', sans-serif; line-height:30px; font-weight:lighter;}
h5{ margin:0 0 10px 0; font-size:20px; line-height:24px; font-family: 'Ubuntu', sans-serif; font-weight:lighter;}
h6{ margin:0 0 10px 0; font-size:16px; line-height:24px; font-family: 'Ubuntu', sans-serif; font-weight:lighter;}

.wrapper{margin:0 auto;width:1050px;position:relative;}
.centeralign{ text-align:center;}
.rightalign{ text-align:right;}
.marginbtmnone{ margin-bottom:0px!important;}
.marginrhtnone{ margin-right:0px!important;}
.marginlftnone{ margin-left:0px!important;}
.paddingbtnnone{padding-bottom:0px!important;}
.paddinglftnone{padding-left:0px!important;}
.paddingrhtnone{padding-right:0px!important;}
.floatleft{ float:left!important;}
.floatright{ float:right!important;}

