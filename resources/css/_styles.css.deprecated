/*===== header css ====== */	
header{ margin:0px; padding:15px  0px; background: url(../img/topbg.png) repeat left top; background:rgba(255,255,255,0.9);}
header h1{ float:left; margin:0px;}
header h1 img{ border:none;}
header .headtopright{ float:right; padding-top:15px;}
header .headtopright nav{ float:left; margin-right:10px; margin-top:9px;}
header .headtopright nav ul, .freetrialsignup ul{ margin:0px; padding:0px;}
header .headtopright nav ul li, .freetrialsignup ul li{ list-style:none; float:left; margin-right:15px; position:relative;}
header .headtopright nav ul li a{ text-decoration:none; color:#656565; font-size:15px;}
header .headtopright nav ul li a:hover{ color:#000;}
.freetrialsignup ul li ul{ display:none; z-index:9999;}
.freetrialsignup ul li:hover ul{ display:block; position:absolute; left:0px; width:90px; top:25px; padding:10px 20px;
	background: #fefefe; /* Old browsers */

	background: -moz-linear-gradient(top,  #fefefe 0%, #efefef 100%); /* FF3.6+ */
	
	background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#fefefe), color-stop(100%,#efefef)); /* Chrome,Safari4+ */
	
	background: -webkit-linear-gradient(top,  #fefefe 0%,#efefef 100%); /* Chrome10+,Safari5.1+ */
	
	background: -o-linear-gradient(top,  #fefefe 0%,#efefef 100%); /* Opera 11.10+ */
	
	background: -ms-linear-gradient(top,  #fefefe 0%,#efefef 100%); /* IE10+ */
	
	background: linear-gradient(to bottom,  #fefefe 0%,#efefef 100%); /* W3C */
	
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#fefefe', endColorstr='#efefef',GradientType=0 ); /* IE6-9 */
	-webkit-border-radius: 8px;
	-moz-border-radius: 8px;
	border-radius: 8px;
	-webkit-box-shadow: #d3d3d3 0px 0px 3px;
	-moz-box-shadow: #d3d3d3 0px 0px 3px;
	box-shadow: #d3d3d3 0px 0px 3px;
	behavior: url(../pie/PIE.htc);
	}
.freetrialsignup ul li ul span{ position:absolute; top:-15px; left:10px;}
.freetrialsignup ul li ul li{ float:none; width:100%; border-bottom:1px solid #dedede; padding:0 0 8px 0; margin-bottom:5px; background:url(../img/bullet.png) no-repeat left 7px; padding-left:10px;}
.freetrialsignup ul li ul li a{ text-decoration:none; display:block;}
.freetrialsignup ul li ul li.lastli{ border-bottom:0px; margin-bottom:0px; padding-bottom:0px;}
.freetrialsignup{ float:left; margin-right:10px; margin-top:8px;}
.freetrialsignup ul li{ margin-right:5px;}
.topsubscribe{ border:1px solid #eaeaea; float:left; line-height:16px; background:white; padding:4px; -webkit-border-radius: 4px;
-moz-border-radius: 4px;
border-radius: 4px;behavior: url(../pie/PIE.htc);}
.topsubscribe fieldset{ margin:0px!important;}
.topsubscribe input[type="text"]{ background:white; border:none; color:#b8b8b8;}
.topsubscribe input[type="button"]{ padding:5px 3px 2px 3px; line-height:18px; background:#c7c7c7; color:#fff; text-transform:uppercase; font-size:12px; border:none; -webkit-border-radius: 6px;
-moz-border-radius: 6px;
border-radius: 6px;behavior: url(../pie/PIE.htc);}
.absolute{ position:absolute; top:0px; left:0px; width:100%; z-index:999;}
		
/*===== section css ====== */
section#content{}
.bannerholder{ position:relative; z-index:1; min-width:1050px;}
.homebanner{ background:url(../img/homebanner.jpg) repeat-x center top;  overflow:hidden; height:669px;}
.trainebanner{ background:url(../img/traine-banner.jpg) repeat-x center top;  overflow:hidden; height:669px;}
.trainerbanner{ background:url(../img/trainer-banner.jpg) repeat-x center top;  overflow:hidden; height:669px;}
.gymbanner{background:url(../img/gym-banner.jpg) repeat-x center top;  overflow:hidden; height:669px;}
.bannerholder img{ min-width:1050px; min-height:100%;}
.bannerholder .wrapper{ padding-top:480px; text-align:center;color:#fff; font-size:40px; text-align:center;}
.homebanner .wrapper{ padding-top:320px}
.wrapper .blackbg{ background:url(../img/blackbg.png) repeat left top; background:rgba(0,0,0,0.9);  display:inline-block; padding:5px 20px; font-weight:bold; font-size:37px;  text-transform:uppercase;}
.wrapper .bluebg{background:url(../img/bluebg.png) repeat left top; background:rgba(47,109,161,0.9);  display:inline-block; padding:5px 30px; font-size:37px;  font-weight:bold; text-transform:uppercase;}


.loginfacebook{ text-align:right; font-size:19px; padding-top:40px; padding-bottom:40px; color:#8e8e8e; position:relative;}
.loginfacebook2{ text-align:right; font-size:19px; padding-top:40px; padding-bottom:40px; color:#8e8e8e; position:relative; margin-left:-45px}
.loginfacebook a{ margin-left:10px;}
.orholder{ position:absolute; color:#a9a9a9; font-size:13px; right:30.5%; background:white; -webkit-border-radius: 90px;
	-moz-border-radius: 90px;
	border-radius: 90px;behavior: url(../pie/PIE.htc);
	padding:8px; z-index:9999;}
.lightpatternbg{ background:url(../img/patbg.gif) repeat left top; padding:30px 0px 50px 0px;}
.introtext{ font-size:23px; margin-top:10px;}
.homeblocks{ float:left; color:#8e8e8e; height:200px; overflow:hidden; font-size:17px; width:28%; padding:3.5% 2%; margin-right:2%; background:white; -webkit-box-shadow: 0px 0px 4px rgba(122, 122, 122, 0.60);
	-moz-box-shadow:    0px 0px 4px rgba(122, 122, 122, 0.60);
	box-shadow:         0px 0px 4px rgba(122, 122, 122, 0.60); behavior: url(../pie/PIE.htc);
